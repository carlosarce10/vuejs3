ement() {\r\n      this.counter++;\r\n    },\r\n  },\r\n  computed: {\r\n    squareCounter() {\r\n      return this.counter * this.counter;\r\n    },\r\n    getTitleCustom() {\r\n      this.newtitle = this.title = !null ? this.title : (this.title = 'Custom');\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\nbutton {\r\n  background-color: rgb(15, 140, 223);\r\n  color: white;\r\n  border-radius: 5px;\r\n  margin: 0, 5px;\r\n  border: 1px solid white;\r\n  cursor: pointer;\r\n  padding: 5px, 15px;\r\n  transition: 0.3s ease-in-out;\r\n  width: 60px;\r\n  height: 40px;\r\n}\r\nbutton:hover {\r\n  background-color: rgb(15, 125, 199);\r\n  transition: 0.3s ease-in-out;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}