{"ast":null,"code":"import PokemonOptions from '@/components/PokemonOptions.vue';\nimport PokemonPicture from '@/components/PokemonPicture.vue';\nimport getPokemonOptions from '@/helpers/getPokemonOptions.js';\nexport default {\n  components: {\n    PokemonOptions,\n    PokemonPicture\n  },\n\n  data() {\n    return {\n      pokemonArr: [],\n      pokemon: null,\n      showPokemon: false,\n      showAnswer: false,\n      message: ''\n    };\n  },\n\n  methods: {\n    async mixPokemonArray() {\n      this.pokemonArr = await getPokemonOptions();\n      const rndId = Math.floor(Math.random() * 4);\n      this.pokemon = this.pokemonArr[rndId];\n    },\n\n    checkAnswer(pokemonId) {\n      this.showPokemon = true;\n      this.showAnswer = true;\n      pokemonId == this.pokemon.id ? this.message = 'Correcto' : this.message = `Fallaste, el pokemon es ${this.pokemon.name}`;\n    },\n\n    newGame() {\n      this.mixPokemonArray();\n      this.showPokemon = false;\n      this.showAnswer = false;\n      this.pokemonArr = [];\n      this.pokemon = null;\n    }\n\n  },\n\n  mounted() {\n    this.mixPokemonArray();\n  }\n\n};","map":{"version":3,"mappings":"AAuBA,OAAOA,cAAP,MAA2B,iCAA3B;AACA,OAAOC,cAAP,MAA2B,iCAA3B;AACA,OAAOC,iBAAP,MAA8B,gCAA9B;AAEA,eAAe;AACbC,YAAU,EAAE;AACVH,kBADU;AAEVC;AAFU,GADC;;AAKbG,MAAI,GAAG;AACL,WAAO;AACLC,gBAAU,EAAE,EADP;AAELC,aAAO,EAAE,IAFJ;AAGLC,iBAAW,EAAE,KAHR;AAILC,gBAAU,EAAE,KAJP;AAKLC,aAAO,EAAE;AALJ,KAAP;AAOD,GAbY;;AAcbC,SAAO,EAAE;AACP,UAAMC,eAAN,GAAwB;AACtB,WAAKN,UAAL,GAAkB,MAAMH,iBAAiB,EAAzC;AAEA,YAAMU,KAAI,GAAIC,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,MAAL,KAAgB,CAA3B,CAAd;AACA,WAAKT,OAAL,GAAe,KAAKD,UAAL,CAAgBO,KAAhB,CAAf;AACD,KANM;;AAOPI,eAAW,CAACC,SAAD,EAAY;AACrB,WAAKV,WAAL,GAAmB,IAAnB;AACA,WAAKC,UAAL,GAAkB,IAAlB;AAEAS,eAAQ,IAAK,KAAKX,OAAL,CAAaY,EAA1B,GACK,KAAKT,OAAL,GAAe,UADpB,GAEK,KAAKA,OAAL,GAAgB,2BAA0B,KAAKH,OAAL,CAAaa,IAAK,EAFjE;AAGD,KAdM;;AAePC,WAAO,GAAG;AACR,WAAKT,eAAL;AACA,WAAKJ,WAAL,GAAmB,KAAnB;AACA,WAAKC,UAAL,GAAkB,KAAlB;AACA,WAAKH,UAAL,GA