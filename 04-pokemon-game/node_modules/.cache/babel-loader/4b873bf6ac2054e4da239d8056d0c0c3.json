{"ast":null,"code":"import PokemonOptions from '@/components/PokemonOptions.vue';\nimport PokemonPicture from '@/components/PokemonPicture.vue';\nimport getPokemonOptions from '@/helpers/getPokemonOptions.js';\nexport default {\n  components: {\n    PokemonOptions,\n    PokemonPicture\n  },\n\n  data() {\n    return {\n      pokemonArr: [],\n      pokemon: null,\n      showPokemon: false,\n      showAnswer: false,\n      message: ''\n    };\n  },\n\n  methods: {\n    async mixPokemonArray() {\n      this.pokemonArr = await getPokemonOptions();\n      const rndId = Math.floor(Math.random() * 4);\n      this.pokemon = this.pokemonArr[rndId];\n    },\n\n    checkAnswer(pokemonId) {\n      this.showPokemon = true;\n\n      if (pokemonId == this.pokemon) {\n        this.message = 'Andas duro, kbron';\n        console.log(pokemonId, this.pokemon);\n      }\n    }\n\n  },\n\n  mounted() {\n    this.mixPokemonArray();\n  }\n\n};","map":{"version":3,"mappings":"AAaA,OAAOA,cAAP,MAA2B,iCAA3B;AACA,OAAOC,cAAP,MAA2B,iCAA3B;AACA,OAAOC,iBAAP,MAA8B,gCAA9B;AAEA,eAAe;AACbC,YAAU,EAAE;AACVH,kBADU;AAEVC;AAFU,GADC;;AAKbG,MAAI,GAAG;AACL,WAAO;AACLC,gBAAU,EAAE,EADP;AAELC,aAAO,EAAE,IAFJ;AAGLC,iBAAW,EAAE,KAHR;AAILC,gBAAU,EAAE,KAJP;AAKLC,aAAO,EAAE;AALJ,KAAP;AAOD,GAbY;;AAcbC,SAAO,EAAE;AACP,UAAMC,eAAN,GAAwB;AACtB,WAAKN,UAAL,GAAkB,MAAMH,iBAAiB,EAAzC;AAEA,YAAMU,KAAI,GAAIC,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,MAAL,KAAgB,CAA3B,CAAd;AACA,WAAKT,OAAL,GAAe,KAAKD,UAAL,CAAgBO,KAAhB,CAAf;AACD,KANM;;AAOPI,eAAW,CAACC,SAAD,EAAY;AACrB,WAAKV,WAAL,GAAmB,IAAnB;;AACA,UAAIU,SAAQ,IAAK,KAAKX,OAAtB,EAA+B;AAC7B,aAAKG,OAAL,GAAe,mBAAf;AACAS,eAAO,CAACC,GAAR,CAAYF,SAAZ,EAAuB,KAAKX,OAA5B;AACF;AACD;;AAbM,GAdI;;AA6Bbc,SAAO,GAAG;AACR,SAAKT,eAAL;AACD;;AA/BY,CAAf","names":["PokemonOptions","PokemonPicture","getPokemonOptions","components","data","pokemonArr","pokemon","showPokemon","showAnswer","message","methods","mixPokemonArray","rndId","Math","floor","random","checkAnswer","pokemonId","console","log","mounted"],"sourceRoot":"","s